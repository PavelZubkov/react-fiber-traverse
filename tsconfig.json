 {
  "extends": "./tsconfig.base.json",
  "exclude": ["node_modules"],
  "include": ["scripts", "src", "test"],
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationDir": null,
    "module": "commonjs",
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "docs",
    "theme": "markdown",
  }
}